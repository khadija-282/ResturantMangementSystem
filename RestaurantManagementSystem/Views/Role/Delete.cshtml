@model RestaurantManagementSystem.Models.RoleViewModel

@{
    ViewBag.Title = "Delete";
}
<section class="content-header">
    <h1></h1>
    <ol class="breadcrumb"></ol>
</section>
<div class="col-sm-12 col-md-8 col-lg-offset-2">
<div class="box box-primary">
    <div class="box-header">
        <h3>Are you sure you want to delete this?</h3>

    </div>

    <div class="box-body">


        
        <div>
            
            <dl class="dl-horizontal">
                <dt>
                    @Html.LabelFor(model => model.LevelId, "User Level", htmlAttributes: new { @class = "control-label " })
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.LevelName)
                </dd>

                <dt>
                    @Html.LabelFor(model => model.DisplayName, htmlAttributes: new { @class = "control-label" })
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.DisplayName)
                </dd>


            </dl>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Delete" class="btn btn-default" /> 
                    @Html.ActionLink("Back to List", "Index", null, htmlAttributes: new { @class = "btn btn-default" })
                </div>
            }
        </div>
        </div>
    </div>
</div>
