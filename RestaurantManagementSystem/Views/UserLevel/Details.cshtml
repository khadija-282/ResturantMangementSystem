@model RestaurantManagementSystem.Models.UserLevel

@{
    ViewBag.Title = "Details";

}
<section class="content-header">
    <h1></h1>
    <ol class="breadcrumb"></ol>
</section>

<div class="col-sm-12 col-md-8 col-lg-offset-2">
    <div class="box box-primary">
        <div class="box-header">
            <h3 class="box-title">User Level Details</h3>

        </div>

        <div class="box-body">

            <div>
               
                <dl class="dl-horizontal">
                    <dt>
                      
                        @Html.LabelFor(model => model.LevelName, "User Level", htmlAttributes: new { @class = "control-label" })
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.LevelName)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.Description)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Description)
                    </dd>

                    <dt>
                        
                        @Html.LabelFor(model => model.IsActive_YN, "Is Active", htmlAttributes: new { @class = "control-label" })
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.IsActive_YN)
                    </dd>

                </dl>
            </div>
            <p>
                @Html.ActionLink("Edit", "Edit", new { id = Model.LevelId }, htmlAttributes: new { @class = "btn btn-default" })
                @Html.ActionLink("Back to List", "Index", null, htmlAttributes: new { @class = "btn btn-default" })
            </p>
        </div>
    </div>
</div>

